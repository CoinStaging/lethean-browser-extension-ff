{
  "name": "Lethean Proxy/VPN Extension",
  "version": "1.0",
  "description": "Your gateway to the Lethean Proxy/VPN network, a secure and anonymous platform where users sell their bandwidth for cryptocurrency.",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html",
    "browser_style": false
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "applications": {
    "gecko": {
      "strict_min_version": "64.0a2"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "proxy",
    "storage",
    "contextMenus",
    "privacy",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "alarms",
    "<all_urls>"
  ],
  "manifest_version": 2
}
