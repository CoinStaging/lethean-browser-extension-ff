{
  "name": "Intense Coin",
  "version": "1.0",
  "description": "Intense Coin description",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html",
    "browser_style": false
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "applications": {
    "gecko": {
      "strict_min_version": "56.0a1"
    }
  },
  "background": {
    "scripts": [
      "sendControll.js"
    ]
  },
  "permissions": [
    "proxy",
    "storage",
    "contextMenus",
    "privacy",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "manifest_version": 2
}
